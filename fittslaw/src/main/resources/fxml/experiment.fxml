<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hska.iiwi.fittslaw.experiment.ExperimentController">
   <center>
      <VBox fx:id="container" alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" BorderPane.alignment="CENTER">
         <children>
      		<Label fx:id="command" text="command label">
               <font>
                  <Font size="64.0" />
               </font></Label>
            <ImageView fx:id="icon" fitHeight="150.0" fitWidth="200.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" />
            <VBox fx:id="textInput" alignment="CENTER" maxWidth="300.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label fx:id="textInputWord" alignment="CENTER" contentDisplay="CENTER" text="text input label" textAlignment="CENTER">
                     <font>
                        <Font size="64.0" />
                     </font>
                  </Label>
                  <TextField fx:id="textInputField" alignment="CENTER">
                     <font>
                        <Font size="64.0" />
                     </font></TextField>
               </children>
            </VBox>
         </children>
      </VBox>
   </center>
</BorderPane>
